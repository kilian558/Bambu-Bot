# Discord Bot Token
# Erstelle einen Bot auf https://discord.com/developers/applications
DISCORD_TOKEN=dein_discord_bot_token_hier

# Discord Channel ID fÃ¼r automatische Status-Updates
# Rechtsklick auf Channel -> "Channel-ID kopieren" (Developer Mode muss aktiviert sein)
DISCORD_CHANNEL_ID=1466007772809400492

# Bambu Lab Drucker Einstellungen
# IP-Adresse deines Bambu Lab Druckers
BAMBU_HOST=192.168.1.xxx

# MQTT Port (Standard: 8883)
BAMBU_PORT=8883

# Bambu Lab Username (Standard: bblp)
BAMBU_USERNAME=bblp

# Access Code (findet man in der Bambu Handy App unter Einstellungen -> Netzwerk -> MQTT)
BAMBU_ACCESS_CODE=dein_access_code_hier

# Seriennummer deines Druckers (findet man auf dem Drucker oder in der App)
BAMBU_SERIAL=deine_seriennummer_hier
